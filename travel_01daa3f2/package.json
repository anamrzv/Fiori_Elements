{
    "name": "travel_01daa3f2",
    "version": "0.0.1",
    "private": true,
    "sapux": true,
    "sapuxLayer": "CUSTOMER_BASE",
    "description": "A Fiori application that shows list of all travels",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "build": "npm run clean && npm run zip",
        "zip": "npx bestzip travel_01daa3f2.zip *",
        "clean": "npx rimraf travel_01daa3f2.zip",
        "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
        "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo"
    },
    "devDependencies": {
        "@ui5/cli": "^2.11.1",
        "@sap/ui5-builder-webide-extension": "^1.1.6",
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1",
        "@sap/ux-ui5-tooling": "1",
        "rimraf": "3.0.2",
        "@sap/ux-specification": "latest",
        "ui5-task-zipper": "^0.3.1",
        "@sap/ux-ui5-fe-mockserver-middleware": "1"
    },
    "ui5": {
        "dependencies": [
            "@sap/ui5-builder-webide-extension",
            "@sap/ux-ui5-tooling",
            "ui5-task-zipper",
            "@sap/ux-ui5-fe-mockserver-middleware"
        ]
    }
